<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<title>{% block title %}{% endblock %}</title>
		<!-- Tell the browser to be responsive to screen width -->
		<meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
		<!-- Bootstrap 3.3.7 -->
		<link rel="stylesheet" href="{{ www('bower/bootstrap/dist/css/bootstrap.min.css') }}">

		{% block custom_css %}{% endblock %}

		<script type="text/javascript">var BASE_URL = "{{ www('') }}";</script>

		<!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
		<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
		<!--[if lt IE 9]>
		<script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
		<script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
		<![endif]-->

		<!-- Google Font -->
	</head>

	{% block body %} {% endblock %}
	<!-- jQuery 3 -->
	<script src="{{ www('bower/jquery/dist/jquery.min.js') }}"></script>
	<!-- Bootstrap 3.3.7 -->
	<script src="{{ www('bower/bootstrap/dist/js/bootstrap.min.js') }}"></script>

	{% block custom_libraries %}{% endblock %}

	{% block page_libraries %}{% endblock %}

</html>